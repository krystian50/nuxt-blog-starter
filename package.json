{
  "name": "nuxt-blog-starter",
  "version": "1.0.0",
  "description": "nuxt-blog-starter application",
  "author": "Krystian Fras",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -r esm --inspect server/index.js --watch server",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "start": "cross-env NODE_ENV=production node -r esm server/index.js",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.4.1",
    "markdown-it": "^10.0.0",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.3.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "frontmatter-markdown-loader": "^3.2.1",
    "husky": "^4.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "push-dir": "^0.4.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0",
    "vue-jest": "^4.0.0-0"
  }
}
